{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/adoxography/revisionable"
        }
    ],
    "require": {
        "php": ">=7.2",
        "backup-manager/laravel": "^1.3",
        "doctrine/dbal": "^2.5",
        "laravel/framework": "5.8.*",
        "laravel/telescope": "^1.0",
        "laravel/tinker": "^1.0",
        "mikealmond/color": "^0.1",
        "spatie/flysystem-dropbox": "^1.0",
        "spatie/laravel-collection-macros": "^4.0",
        "spatie/laravel-permission": "^2.3.2",
        "spatie/laravel-sluggable": "^2.0",
        "srmklive/flysystem-dropbox-v2": "^1.0",
        "symfony/polyfill-iconv": "^1.10",
        "teamtnt/laravel-scout-tntsearch-driver": "^7.0",
        "venturecraft/revisionable": "dev-master"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~8.0",
        "symfony/css-selector": "4.2.*",
        "symfony/dom-crawler": "4.2.*"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "Adoxography\\VerbalExpressions\\": "packages/adoxography/verbal-expressions/src",
            "App\\": "app/"
        },
        "files": [
            "app/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Tests\\": "tests/",
            "Idea\\": ".idea/"
        },
        "files": [
            "tests/helpers/functions.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
      "laravel": {
        "dont-discover": [
          "barryvdh/laravel-debugbar",
          "laravel/dusk"
        ]
      }
    }
}
