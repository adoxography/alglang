{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "laracasts/utilities": "~2.0",
        "netcarver/textile": "3.6.*",
        "backup-manager/laravel": "^1.1",
        "league/flysystem-dropbox": "^1.0",
        "doctrine/dbal": "^2.5",
        "venturecraft/revisionable": "1.*",
        "laravel/tinker": "^1.0",
        "laravel/dusk": "^1.0",
        "h4cc/wkhtmltopdf-i386": "0.12.x",
        "teamtnt/laravel-scout-tntsearch-driver": "^2.0",
        "mikealmond/color": "0.0.4",
        "spatie/flysystem-dropbox": "^1.0",
        "rap2hpoutre/laravel-log-viewer": "^0.10.4",
        "spatie/laravel-permission": "^2.3.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "barryvdh/laravel-ide-helper": "^2.4"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Algling\\SS\\": "packages/algling/ss/src",
            "Algling\\Words\\": "packages/algling/words/src",
            "Algling\\Verbals\\": "packages/algling/verbals/src",
            "Algling\\Nominals\\": "packages/algling/nominals/src",
            "Algling\\Phonology\\": "packages/algling/phonology/src",
            "Adoxography\\VerbalExpressions\\": "packages/adoxography/verbal-expressions/src",
            "App\\": "app/"
        },
        "files": [
            "app/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Tests\\": "tests/",
            "Algling\\Log\\": "packages/algling/log/src",
            "Idea\\": ".idea/"
        },
        "files": [
            "tests/helpers/functions.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
